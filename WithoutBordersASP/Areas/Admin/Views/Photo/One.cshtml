@model IEnumerable<WithoutBorders.Data.Entities.Photo>
<!-- Фото -->
<div class="photoEdit">
    <div class="addPhoto">
        <a href="">Зберегти все</a>
        <a>Добавити фото</a>
    </div>
    <div class="grid">
        <div class="grid-sizer"></div>
        @if (Model.Any())
        {
            foreach (var photo in Model)
            {
                <div class="grid-item">
                    <img src="@photo.Url" alt="">
                    <input asp-for="@photo.Url" type="text">
                    <a asp-action="Delete" asp-route-id="@photo.Id">Видалити</a>
                </div>
            }
        }
    </div>
</div>